<section class="card" id="mainContent">

	<div class="card-header card-header-adjustments">
		<span class="cat__core__title pull-left"> <strong class="page-title">Dashboard</strong>
		<span class="time-text">(Last updated on 12-02-2018, 4:30 pm)</span>
		</span>
		<span class="pull-right">
			<i class="fa fa-refresh icon-color"></i>
		</span>
	</div>
	<div class="card-block db-block"
		style="padding-left: 0px; padding-right: 0px; padding-bottom: 0px;">
	<div class="row">
		<div class="col-lg-12" style="padding-right:0px;">
			<div class="row col-md-12" style="padding-right:0px;">
			  <div class="col-md-3  main-dash">
				<div class="purchase-content">
					<div class="purchase-header">
						<div class="pull-left">
						<h1 class="price-order">120</h1>
							<h5>Pending Sales Order</h5>
						</div>
						<div class="pull-right dboard-icon">
							<img src="assets/modules/dummy-assets/common/img/dashboard-icons/excel-icon.png" />
						</div>
					</div>
				</div>
			</div>
			<div class="col-md-3  main-dash">
				<div class="purchase-content">
					<div class="sales-header">
						<div class="pull-left">
						<h1 class="price-order">120</h1>
							<h5>Pending Delivery</h5>
						</div>
						<div class="pull-right dboard-icon">
							<img src="assets/modules/dummy-assets/common/img/dashboard-icons/sales-order-icon.png" />
						</div>
					</div>
				</div>
					</div>
			
			<div class="col-md-3  main-dash">
				<div class="purchase-content">
					<div class="pending-header">
						<div class="pull-left">
						<h1 class="price-order"><i class="fa fa-rupee"></i>200</h1>
							<h5>Pending Invoice</h5>
						</div>
						<div class="pull-right dboard-icon">
							<img src="assets/modules/dummy-assets/common/img/dashboard-icons/pending-invoice-db.png" />
						</div>
					</div>
				</div>
			</div>
			<div class="col-md-3  main-dash" style="padding-right:0px;">
				<div class="purchase-content">
					<div class="notifiction-header">
						<div class="pull-left">
						<h1 class="price-order"> 100</h1>
							<h5>Pending GR</h5>
						</div>
						<div class="pull-right dboard-icon">
							<img src="assets/modules/dummy-assets/common/img/dashboard-icons/pending-gr-db.png" />
						</div>
					</div>
				</div>
			</div>
			  </div>
		<div class="col-md-12 row" style="padding-right:0px;">
		<div class="col-md-6" style="padding-left:0px;background:#fff;">
		<div class="graph-block">
		<h6>Sales</h6>
		<label style="margin-bottom:3px;"><i class="fa fa-rupee" style="margin-right:4px"></i>4,35,00,400</label>
		<small style="display:block;margin-bottom:5px;">This Financial Year Month</small>
		    <canvas baseChart 
		                [data]="lineChartData"
		                [labels]="lineChartLabels"
		                [options]="lineChartOptions"
		                [colors]="lineChartColors"
		                [chartType]="lineChartType"
		                (chartHover)="chartHovered($event)"
		                [legend]="lineChartLegend"
		                (chartClick)="chartClicked($event)"></canvas>
		      </div>
		  </div>
		  <div class="col-md-6" style="padding-left:0px;padding-right:0px;background:#fff;">
		  <div class="graph-block">
			<h6 style="margin-bottom:0px;">Purchase</h6>
			<label style="margin-bottom:3px;"><i class="fa fa-rupee" style="margin-right:4px"></i>5,05,00,500</label>
			<small style="display:block;margin-bottom:5px;">This Financial Year Month</small>
		    <canvas baseChart 
		                [data]="lineChartData"
		                [labels]="lineChartLabels"
		                [options]="lineChartOptions"
		                [colors]="lineChartColors1"
		                [chartType]="lineChartType"
		                [legend]="lineChartLegend"
		                (chartHover)="chartHovered($event)"
		                (chartClick)="chartClicked($event)"></canvas>
		    </div>
		  </div>
		</div>
		<div class="clear-fix"></div>
		<div class="col-md-12 row mt-3" style="padding-right:0px;">
			<div class="col-md-6" style="padding-left:0px;background:#fff;">
				<div class="graph-block">
				<h6>Sales Invoices</h6>
				<label style="margin-bottom:3px;border-bottom:2px solid #ccc;width:98%;">
					<i class="fa fa-rupee" style="margin-right:4px"></i>
					<span style="font-size:13px;">5,55,170 Unpaid / 2,00,000 Paid</span>				
				</label>
				<div class="col-md-12 row" style="padding:0px;">
					<div class="col-md-6">
					<div class="price-sale">1,69,483</div>
					<div style="margin-top:-5px;">Overdue</div>
					</div>
					<div class="col-md-6" style="text-align:right;padding-right:0px;">
					<div>385,689</div>
					<div style="margin-top:-5px;">Not Due Yet</div>
					</div>
				</div>
				<div class="col-md-12 mb-3" style="padding-left:0px;">
					<div class="progress">
							<div class="progress-bar bg-primary" role="progressbar"
								id="actSavedLoclCnt" aria-valuenow="15" aria-valuemin="0"
								aria-valuemax="100" style="width:100px"></div>
						</div>
				</div>
				</div>
			</div>
			<div class="col-md-6" style="padding-left:0px;background:#fff;">
				<div class="graph-block">
				<h6>Purchase Invoices</h6>
				<label style="margin-bottom:3px;border-bottom:2px solid #ccc;width:98%;">
					<i class="fa fa-rupee" style="margin-right:4px"></i>
					<span style="font-size:13px;">5,55,170 Unpaid / 2,00,000 Paid</span>				
				</label>
				<div class="col-md-12 row" style="padding:0px;">
					<div class="col-md-6">
					<div class="price-purchase">1,50,000</div>
					<div style="margin-top:-5px;">Overdue</div>
					</div>
					<div class="col-md-6" style="text-align:right;padding-right:0px;">
					<div class="range-purchase">3,00,000</div>
					<div style="margin-top:-5px;">Not Due Yet</div>
					</div>
				</div>
				<div class="col-md-12 mb-3" style="padding-left:0px;">
					<mat-progress-bar mode="determinate" value="40"></mat-progress-bar>
				</div>
				</div>
			</div>
		</div>
			</div>
		</div>
		</div>
</section>