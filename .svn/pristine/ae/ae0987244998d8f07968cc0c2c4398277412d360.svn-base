}
.price-order{
	font-weight:600;
	font-family:'Open Sans';
	margin-bottom:0px;
	font-size:28px;
}
.purchase-content{
	background:#F9FAFB;
}
.purchase-footer{
	background:#F9FAFB;
	border:1px solid #e4e9f0;
	padding:10px 10px 15px;
	border-top:0px;
}
.right-option{
	color:#CF4040;
	margin-top: -8px;
}
.main-dash{
    padding: 0px;
    padding-right:15px;
    margin-bottom:15px;
}
