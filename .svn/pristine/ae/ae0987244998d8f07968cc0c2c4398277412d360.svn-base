.purchase-header{
	background: linear-gradient(#077598 , #6ebed8 12%, #077598 90%);
    color: #fff;
    padding: 5px 10px;
    min-height: 60px;
    padding-right:5px;
}
.sales-header{
	background: linear-gradient(#e4ae09 , #ffe79f 12%, #e4ae09 90%);
    color: #fff;
    padding: 5px 10px;
    min-height: 60px;
    padding-right:5px;
}
.pending-header{
	background: linear-gradient(#43A7A1 , #50D5CE 12%, #43A7A2 90%);
    color: #fff;
    padding: 5px 10px;
    min-height: 60px;
    padding-right:5px;
}
.payable-header{
	background: linear-gradient(#418135 , #5C9433 12%, #418135 90%);
