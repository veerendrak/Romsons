@media only screen and (max-width: 1300px){
	.donut-right-block{
	margin-left:5%; margin-top:5%;
}
.db-text{
		text-overflow: ellipsis;
	    width: 100px;
	    overflow: hidden;
	    white-space: nowrap;
	}
	}
@media only screen and (max-width: 981px){
	.db-text{
		text-overflow: ellipsis;
	    width: 100px;
	    overflow: hidden;
	    white-space: nowrap;
	}
}
@media only screen and (max-width: 981px){
	.db-text{
		text-overflow: ellipsis;
	    width: 60px;
	    overflow: hidden;
	    white-space: nowrap;
	}
	
}
@media only screen and (max-width: 948px){
	.main-dash{
		max-width: 50% !important;
		padding-right:10px;
	}
}
@media only screen and (max-width: 768px){
	.main-dash{
		max-width: 50% !important;
		padding-right:10px;
	}
	.db-text{
		text-overflow: ellipsis;
	    width: 130px;
	    overflow: hidden;
	    white-space: nowrap;
	}
	.sales-block{
	padding-right:0px;
	}
}
@media only screen and (max-width: 498px){
	.main-dash{
		max-width: 100% !important;
		padding-right:0px;
	}
	.db-text{
		text-overflow: ellipsis;
	    width: 180px;
	    overflow: hidden;
	    white-space: nowrap;
	}
}
	