@media only screen and (max-width: 767px){
.cat__wizard .steps ul li:before, .cat__wizard .steps ul li:after {
    position: absolute;
    content: '';
    display: block;
    width: 97%;
    top: 0.7rem;
    right: 0;
    height: 0.60rem;
    background: #006666;
    z-index: 1;
}
.cat__wizard{
	margin-left:2% !important;
}
.cat__wizard .steps ul li.last:after{width:0 !important;}
.cat__wizard .steps ul li.last:before{width:0 !important;}
.start{margin-left:0px !important;}
}
.start{margin-left:8px !important;}
.cat_wizard_steps_title {
    display: block;
    text-align: left;
    color:#02615c;
    font-family: 'Open Sans';
    font-size: 14px;
    font-weight:500;
}
.cat__wizard .steps ul li{
	margin-bottom:0px !important;
}
.cat__wizard .steps ul{
	width:100%;
	margin-bottom:0px;
}
.cat__wizard{
	width:100% !important;
	margin: 0 6%;
	padding-left: 6%;
    padding-right: 3%;
}
.modal-body .form-control {
	width: auto !important;
}
.error{
	color:red;
}
.right-block{
	float:right;
}
.table thead th{
	min-width:130px !important;
}
table {
		display:block;
		width:100%;
		overflow-x:auto;
		overflow-y:hidden;
	}
	.text-name-elipsis{
	overflow:hidden;
	width:230px;
	text-overflow:ellipsis;
	white-space:nowrap;
}