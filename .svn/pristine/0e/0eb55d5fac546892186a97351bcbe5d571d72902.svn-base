@media only screen and (max-width: 767px){
.cat__wizard .steps ul li:before, .cat__wizard .steps ul li:after {
    position: absolute;
    content: '';
    display: block;
    width: 97%;
    top: 0.9rem;
    right: 0;
    height: 0.80rem;
    background: rgb(34, 190, 239);
    z-index: 1;
}
}

.cat_wizard_steps_title {
    display: block;
