@media only screen and (max-width: 767px){
.cat__wizard .steps ul li:before, .cat__wizard .steps ul li:after {
    position: absolute;
    content: '';
    display: block;
    width: 97%;
    top: 0.7rem;
    right: 0;
    height: 0.60rem;
    background: rgb(34, 190, 239);
    z-index: 1;
}
}

.cat_wizard_steps_title {
    display: block;
    text-align: left;
    color:#02615c;
    font-family: OpenSans;
    font-size: 14px;
    font-weight:600;
}
.cust-det-title{
	font-weight: bold;
    color: black;
    margin-left: 7px;
}
.clear-fix{clear: both;}
.cust-form-block{
	padding-left:0px;
	border-bottom: 2px dashed #ccc; margin-left: 0px;
}