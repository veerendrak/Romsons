@media only screen and (max-width: 767px){
.cat__wizard .steps ul li:before, .cat__wizard .steps ul li:after {
    position: absolute;
    content: '';
    display: block;
    width: 97%;
    top: 0.7rem;
    right: 0;
    height: 0.60rem;
    background: rgb(34, 190, 239);
    z-index: 1;
}
}

