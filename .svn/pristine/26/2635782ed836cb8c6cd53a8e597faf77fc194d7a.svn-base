.purchase-header{
	background: linear-gradient(#077598 , #6ebed8 33%, #077598 90%);
    color: #fff;
    padding: 5px 10px;
    min-height: 60px;
}
.sales-header{
	background: linear-gradient(#F9CA3C , #FCDB74 33%, #F9CA3C 90%);
    color: #fff;
    padding: 5px 10px;
    min-height: 60px;
}
.pending-header{
	background: linear-gradient(#43A7A1 , #50D5CE 33%, #43A7A2 90%);
    color: #fff;
    padding: 5px 10px;
    min-height: 60px;
}
.payable-header{
	background: linear-gradient(#418135 , #5C9433 33%, #418135 90%);
