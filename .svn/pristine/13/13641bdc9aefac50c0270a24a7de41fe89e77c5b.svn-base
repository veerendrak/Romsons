			<li class="cat__menu-left__item cat__menu-left__submenu" [routerLinkActive]="['cat__menu-left__item--active']"  *ngFor = "let list of leftMenu" 
            >
               <div *ngIf="list.childLength; then thenBlock else elseBlock"></div>
					<ng-template #thenBlock>
			                <a href="javascript: void(0);" *ngIf="list.permissionLevel==1">
			                    <i class="fa {{list.menuIcon}} cat_menu_icons_left"></i>
			                    {{list.parentObjName}}
			                </a> 
					<ul class="cat__menu-left__list">
                    
                    <li class="cat__menu-left__item menu-custom-left" [routerLinkActive]="['cat__menu-left__item--active']" *ngFor = "let childlist of list.childObjs"
                    >
                        <a [routerLink]="[childlist.childUrl | lowercase]" *ngIf="childlist.permissionLevel==1 && childlist.childUrl != 'MySupportTickets' && childlist.childUrl != 'HelpDocs' && childlist.childUrl != 'ASPTaxCommunity' && childlist.childUrl != 'ManageOTP' && childlist.childUrl != 'Reportabug' && childlist.childUrl != 'Contactus'" >
                            <i class="fa {{childlist.menuIcon}} cat_menu_icons_left"></i>
                           {{childlist.objName}}
                        </a>
                        <a href="https://support.asptax.com" target="_blank" *ngIf="childlist.permissionLevel==1 && childlist.childUrl == 'MySupportTickets'">
                            <i class="fa {{childlist.menuIcon}} cat_menu_icons_left"></i>
                           {{childlist.objName}}
                        </a> 
                         <a href="https://support.asptax.com" target="_blank" *ngIf="childlist.permissionLevel==1 && childlist.childUrl == 'Reportabug'">
                            <i class="fa {{childlist.menuIcon}} cat_menu_icons_left"></i>
                           {{childlist.objName}}
                        </a> 
                        
                       <a href="mailto:contactus@asptax.com" target="_blank" *ngIf="childlist.permissionLevel==1 && childlist.childUrl == 'Contactus'">
                            <i class="fa {{childlist.menuIcon}} cat_menu_icons_left"></i>
                           {{childlist.objName}}
                        </a> 
                        <a href="https://help.asptax.com/" target="_blank" *ngIf="childlist.permissionLevel==1 && childlist.childUrl == 'HelpDocs'">
                       	
                           <i class="fa {{childlist.menuIcon}} cat_menu_icons_left"></i>
                          {{childlist.objName}}
                       </a>
                       <a href="https://groups.google.com/d/forum/asptax" target="_blank" *ngIf="childlist.permissionLevel==1 && childlist.childUrl == 'ASPTaxCommunity'">
                           <i class="fa {{childlist.menuIcon}} cat_menu_icons_left"></i>
                          {{childlist.objName}}
                       </a>
                       <a href="javascript:void(0)"  (click)="updateOtp()" *ngIf="childlist.permissionLevel==1 && childlist.childUrl == 'ManageOTP'">
                           <i class="fa {{childlist.menuIcon}} cat_menu_icons_left"></i>
                          {{childlist.objName}}
                       </a>
                        
                    </li>
                    
                    
                </ul>
                
					</ng-template>
					<ng-template #elseBlock>
					 <a [routerLink]="[list.menuUrl | lowercase]"  class="no-sub-menu">
			                    <i class="fa {{list.menuIcon}} cat_menu_icons_left"></i>
			                    {{list.parentObjName}}
			                </a>
					</ng-template> 
                </li>
		</ul>
		
