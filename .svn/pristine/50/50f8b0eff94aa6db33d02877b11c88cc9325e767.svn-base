<section class="card" id="mainContent" style="padding-left:0px; padding-right:0px;padding-bottom:0px;">
<div style="padding-left:15px; padding-right:15px;">
	<div class="card-header card-header-adjustments">
		<span class="cat__core__title"> <strong class="page-title">Outbound Delivery</strong>
		</span>
		<span class="pull-right" style="margin-right:-10px;">
			<button class="btn login-button">Post Goods issue</button>
		</span>
	</div>
	<div class="card-block db-block" style="padding-left:0px; padding-right:0px;padding-bottom:0px;">
		<div class="row">
			<div class="col-lg-12">
				<div class="row">
					<!-- new changes -->
					<div class="outbound-content col-md-12">
					<form [formGroup]="deliveryForm">
					 <div class="delivery-form-block col-md-12 row pb-5">
						<div class="col-md-3">
							<mat-form-field
							      [floatLabel]="deliveryForm.value.floatLabel">
							      <mat-label>Shipping Point</mat-label>
							    <input matInput placeholder="shipping point" required>
							  </mat-form-field>
						</div>
						<div class="col-md-3">
							<mat-form-field
							      [floatLabel]="deliveryForm.value.floatLabel">
							      <mat-label>Select Date</mat-label>
							    <input matInput placeholder="select date" required>
							  </mat-form-field>
						</div>
						<div class="col-md-3">
							<mat-form-field
							      [floatLabel]="deliveryForm.value.floatLabel">
							      <mat-label>Order Number</mat-label>
							    <input matInput placeholder="order number" required>
							  </mat-form-field>
						</div>
						<div class="col-md-3">
							<mat-form-field
							      [floatLabel]="deliveryForm.value.floatLabel">
							      <mat-label>Ship to party name</mat-label>
							    <input matInput placeholder="party name" required>
							  </mat-form-field>
						</div>
						<div class="clear-fix"></div>
						<div class="col-md-3">
							<mat-form-field
							      [floatLabel]="deliveryForm.value.floatLabel">
							      <mat-label>Doc Date</mat-label>
							    <input matInput placeholder="doc date" required>
							  </mat-form-field>
						</div>
						<div class="col-md-3">
							<mat-form-field
							      [floatLabel]="deliveryForm.value.floatLabel">
							      <mat-label>GI Date</mat-label>
							    <input matInput placeholder="gi date" required>
							  </mat-form-field>
						</div>
						<div class="col-md-3">
							<mat-form-field
							      [floatLabel]="deliveryForm.value.floatLabel">
							      <mat-label>Number of packages</mat-label>
							    <input matInput placeholder="num of packages" required>
							  </mat-form-field>
						</div>
						<div class="col-md-3">
							<mat-form-field
							      [floatLabel]="deliveryForm.value.floatLabel">
							      <mat-label>Total weight</mat-label>
							    <input matInput placeholder="total weight" required>
							  </mat-form-field>
						</div>
					</div>
					</form>
					</div>
					<div class="col-md-12 mt-5">
							<div class="stock-table">
								<table class="table table-hover nowrap editable-table checkBoxTable"
								width="100%" id="outbound-delivery-table" style="overflow-x:auto;">
								<thead>
									<tr>
										<th style="width:50px;text-align:center;"><mat-checkbox></mat-checkbox></th>
										<th style="width:100px;">Material Code</th>
										<th>Order Qty</th>
										<th style="width:100px;">Picking Qty</th>
										<th>Description</th>
										<th>UOM</th>
									</tr>
								</thead>
								<tbody>
								<tr>
									<td align="center"><mat-checkbox></mat-checkbox></td>
									<td>
										<input type="text" class="outbound-input"/>
									</td>
									<td></td>
									<td align="right">
										<i class="fa fa-plus-circle add-qty"></i>
									</td>
									<td>
										
									</td>
									<td>
									</td>
								</tr>
								<tr>
									<td align="center"><mat-checkbox></mat-checkbox></td>
									<td>
									</td>
									<td></td>
									<td>
									</td>
									<td>
										
									</td>
									<td>
									</td>
								</tr>
								<tr>
									<td align="center"><mat-checkbox></mat-checkbox></td>
									<td>
									</td>
									<td></td>
									<td>
									</td>
									<td>
										
									</td>
									<td>
									</td>
								</tr>
								</tbody>
								</table>
							</div>						
						</div>
					<!-- end chnges -->
				</div>
			</div>
		</div>
	</div>
	</div>
	<div class="col-md-12 outbound-footer row mt-4" style="margin-left:0px;">
		<div class="col-md-10 row">
		<div class="col-md-2">
			<mat-checkbox>Select all</mat-checkbox>
		</div>
		<div class="col-md-3" style="margin-left:-40px;">
			<button class="btn login-button">
			<i class="fa fa-user" style="margin-right:4px;"></i> Batch Split</button>
		</div>
		</div>
		<div class="col-md-2 row" style="margin-left:15px;">
			<div class="col-md-6">
			<button class="btn login-button">
			<i class="fa fa-save" style="margin-right:4px;"></i>Save</button>
			</div>
			<div class="col-md-6">
			<button class="btn cancel-button">
			<i class="fa fa-times-circle" style="margin-right:4px;"></i>Cancel</button>
			</div>
		</div>
	</div>
</section>